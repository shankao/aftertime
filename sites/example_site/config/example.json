{
	"site": "example_site",
	"webtitle": "Example site",
	"init_app": "example_app",
	"logs": "logs",
	"apps": {
		"example_app": {
			"init_page": "example_page",
			"pages": {
				"example_page": {
					"title": "Page example",
					"params": {
						"int": {
							"filter": "int",
							"filter_options": {
								"min_range": "5",
								"max_range": "10"
							},
							"filter_flags": "FILTER_FLAG_ALLOW_HEX|FILTER_FLAG_ALLOW_OCTAL"
						},
						"regexp": {
							"filter": "validate_regexp",
							"filter_options": {
								"regexp": "/^[a|b]?c$/"
							}
						},
						"callback": {
							"filter": "callback",
							"filter_options": {
								"callback": "my_validator"
							}
						}
					}
				}
			}
		}
	}
}
